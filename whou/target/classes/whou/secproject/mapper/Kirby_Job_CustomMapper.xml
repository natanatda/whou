<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-Mapper.dtd">
<mapper namespace="whou.secproject.mapper.Kirby_Job_CustomMapper">
	<select id="selectKirby" resultType="whou.secproject.component.Kirby_Job_CustomDTO">
		select * from kirby_job_custom where num=#{num}
	</select>
	<select id="selectKirby2" resultType="whou.secproject.component.Kirby_Job_CustomDTO">
		select * from kirby_job_custom order by num
	</select>
</mapper>